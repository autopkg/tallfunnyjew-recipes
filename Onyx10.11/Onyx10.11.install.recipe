    <key>ParentRecipe</key>
    <string>com.github.tallfunnyjew.pkg.Onyx1011</string>
    <key>Process</key>
    <array>
        <dict>
            <key>Processor</key>
            <string>Installer</string>
			<key>Arguments</key>
            <dict>
                <key>pkg_path</key>
                <string>%RECIPE_CACHE_DIR%/%NAME%*</string>
            </dict>
        </dict>
    </array>
